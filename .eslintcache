sole.log(err);\n        if (err.response.status === 400 || err.response.status === 404) {\n          toast.error('Could not create product, contact support');\n        }\n      });\n  };\n\n  const handleChange = (e) => {\n    setValues({ ...values, [e.target.name]: e.target.value });\n    // console.log(e.target.name, '-------', e.target.value);\n  };\n\n  return (\n    <>\n      <AdminNav />\n      <div className=\"admin-container productCreate\">\n        <h1> Create Product </h1>\n        <form onSubmit={handleSubmit}>\n          <div className=\"category--form\">\n            <input\n              type=\"text\"\n              name=\"title\"\n              className=\"product-form\"\n              value={title}\n              onChange={handleChange}\n              placeholder=\"Title\"\n              required=\"true\"\n            />\n            <br />\n            <textarea\n              name=\"description\"\n              className=\"product-form\"\n              cols=\"30\"\n              rows=\"10\"\n              value={description}\n              onChange={handleChange}\n              placeholder=\"Description\"\n              required=\"true\"></textarea>\n\n            <br />\n            <input\n              type=\"number\"\n              name=\"price\"\n              className=\"product-form\"\n              value={price}\n              onChange={handleChange}\n              placeholder=\"Price\"\n              required=\"true\"\n            />\n            <br />\n            <label>Shipping </label>\n            <select\n              name=\"shipping\"\n              className=\"product-select\"\n              onChange={handleChange}\n              required=\"true\">\n              <option>Please select:</option>\n              <option value=\"No\">No</option>\n              <option value=\"Yes\">Yes</option>\n            </select>\n            <br />\n            <input\n              type=\"number\"\n              name=\"quantity\"\n              className=\"product-form\"\n              value={quantity}\n              onChange={handleChange}\n              placeholder=\"Quantity\"\n              required=\"true\"\n            />\n            <br />\n            <label>Color </label>\n            <select name=\"color\" className=\"product-select\" onChange={handleChange} required=\"true\">\n              <option>Please select:</option>\n              {colors.map((c) => (\n                <option key={c} value={c}>\n                  {c}\n                </option>\n              ))}\n            </select>\n            <br />\n            <label>Brand </label>\n            <select name=\"brand\" className=\"product-select\" onChange={handleChange} required=\"true\">\n              <option>Please select:</option>\n              {brands.map((b) => (\n                <option key={b} value={b}>\n                  {b}\n                </option>\n              ))}\n            </select>\n            <br />\n            <button>Create product</button>\n          </div>\n        </form>\n      </div>\n    </>\n  );\n};\n\nexport default ProductCreate;\n","/Users/deadangel/Documents/udemy/MERN_app/client/src/pages/admin/coupons.jsx",[],{"ruleId":"173","severity":1,"message":"174","line":3,"column":10,"nodeType":"175","messageId":"176","endLine":3,"endColumn":22},{"ruleId":"173","severity":1,"message":"177","line":3,"column":24,"nodeType":"175","messageId":"176","endLine":3,"endColumn":38},{"ruleId":"178","severity":1,"message":"179","line":17,"column":6,"nodeType":"180","endLine":17,"endColumn":8,"suggestions":"181"},{"ruleId":"182","replacedBy":"183"},{"ruleId":"184","replacedBy":"185"},{"ruleId":"182","replacedBy":"186"},{"ruleId":"184","replacedBy":"187"},{"ruleId":"182","replacedBy":"188"},{"ruleId":"184","replacedBy":"189"},{"ruleId":"182","replacedBy":"190"},{"ruleId":"184","replacedBy":"191"},{"ruleId":"182","replacedBy":"192"},{"ruleId":"184","replacedBy":"193"},{"ruleId":"182","replacedBy":"194"},{"ruleId":"