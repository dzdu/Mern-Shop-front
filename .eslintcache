 => {\n  const { user } = useSelector((state) => ({ ...state }));\n  const [name, setName] = React.useState('');\n  const [loading, setLoading] = React.useState(false);\n\n  React.useEffect(() => {\n    loadCategory();\n  }, []);\n\n  const loadCategory = () =>\n    getCategory(match.params.slug)\n      .then((categories) => {\n        setName(categories.data.name);\n        //console.log(categories);\n      })\n      .catch((err) => console.log(err));\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    //console.log(name);\n    setLoading(true);\n    updateCategory(match.params.slug, { name }, user.token)\n      .then((res) => {\n        // console.log(res)\n        setLoading(false);\n        setName('');\n        toast.success(`\"${res.data.name}\" is updated :)`);\n        history.push('/admin/category');\n      })\n      .catch((err) => {\n        console.log(err);\n        setLoading(false);\n        if (err.response.status === 400) toast.error(err.response.data + '' + err.message);\n      });\n  };\n\n  return (\n    <>\n      <AdminNav />\n      {!loading ? (\n        <div className=\"admin-container category\">\n          <h2 style={{ textTransform: 'uppercase' }}>Update Category {match.params.slug}</h2>\n          <div className=\"form\">\n            <CategoryForm handleSubmit={handleSubmit} name={name} setName={setName} />\n          </div>\n        </div>\n      ) : (\n        <div className=\"loader\">Loading...</div>\n      )}\n    </>\n  );\n};\n\nexport default CategoryUpdate;\n",["144","145"],"/Users/deadangel/Documents/udemy/MERN_app/client/src/functions/category.js",[],"/Users/deadangel/Documents/udemy/MERN_app/client/src/pages/admin/categories/category.jsx",[],["146","147"],"/Users/deadangel/Documents/udemy/MERN_app/client/src/components/forms/categoryForm.jsx",[],"/Users/deadangel/Documents/udemy/MERN_app/client/src/index.js",[],["148","149"],"/Users/deadangel/Documents/udemy/MERN_app/client/src/pages/index.js",[],["150","151"],"/U